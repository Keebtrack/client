{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "indent": 0,
        "react/jsx-closing-tag-location": 0,
        "eol-last": 0,
        "comma-dangle": 0,
        "arrow-parens": 0,
        "global-require": 0,
        "react/prefer-es6-class": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-curly-spacing": [ 2, "always" ],
        "react/jsx-indent": 0
    },
    "globals": {
        "document": true
    }
}